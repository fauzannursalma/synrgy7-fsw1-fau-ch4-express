<table>
  <thead>
    <tr>
      <th>id</th>
      <th>name</th>
      <th>username</th>
      <th>email</th>
    </tr>
  </thead>
  <tbody>
    <% people.forEach(person => { %>
    <tr>
      <td><%= person.id %></td>
      <td><%= person.name %></td>
      <td><%= person.username %></td>
      <td><%= person.email %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
